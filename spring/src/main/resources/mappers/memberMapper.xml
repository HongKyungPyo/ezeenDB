<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="mappers.memberMapper">
  
   <insert id="memberInsert" parameterType="model.MemberDTO">
   		insert into member(MEMB_ID,MEMB_NAME,MEMB_PW,MEMB_ADDR,MEMB_PHONE_NUMBER,
   		MEMB_EMAIL,MEMB_BIRTH,MEMB_GENDER,MEMB_ENTER_DATE,MEMB_DATE,MEMB_CONFIRM,POST_NUMBER,
   		DETAIL_ADDR)
   		value(#{membId},,#{membName}#{membPw},#{membAddr},#{membPhoneNumber},
   			#{membEmail},#{membBirth},#{membGender},sysdate,#{membConfirm},
   			#{postNumber},#{detailAddr})
   </insert>
  
  <select id="memList" resultType="model.MemberDTO">
  		select MEMB_ID membId,MEMB_NAME membName,MEMB_PW membPw,MEMB_ADDR membAddr,MEMB_PHONE_NUMBER membPhoneNumber,
   		MEMB_EMAIL membEamil,MEMB_BIRTH membBirth,MEMB_GENDER membGender,MEMB_ENTER_DATE membEnterDate,MEMB_DATE membDate,MEMB_CONFIRM membConfirm,POST_NUMBER postNumber,
   		DETAIL_ADDR detailAddr
   		from member
  
  </select>
  
  
  
  
</mapper>